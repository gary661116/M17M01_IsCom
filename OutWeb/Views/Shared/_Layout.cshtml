                                {
                                    cateb_id = d_menub.Rows[i]["cate_b_id"].ToString();
                                    cateb_name = d_menub.Rows[i]["cate_name"].ToString();
                                    //--Temp檔--建立---------//
                                    DataTable menus;
                                    //----------------------------------------------------------------------//
                                    DataRow[] menus_Rows = d_menus.Select("cate_b_id=" + cateb_id);
                                    //----------------------------------------------------------------------//
                                    if (menus_Rows.Any())
                                    {
                                        menus = menus_Rows.CopyToDataTable();
                                    }
                                    else
                                    {
                                        menus = d_menus.Clone();
                                        menus.Clear();
                                    }

                                    if (menus.Rows.Count > 0)
                                    {
                                        cc_menus += "<li class=\"dropdown\">";
                                        cc_menus += "    <a class=\"dropdown-toggle\" href=\"" + Url.Content("~/Prod?cateb_id=" + cateb_id) + "\">";
                                        cc_menus += "    " + cateb_name + " ";
                                        cc_menus += "    <i class=\"fa fa-angle-down\"></i>";
                                        cc_menus += "    </a>";
                                        cc_menus += "    <ul class=\"dropdown-menu\">";
