                                        {
                                            cates_id = menus.Rows[j]["cate_s_id"].ToString();
                                            cates_name = menus.Rows[j]["cate_name"].ToString();
                                            cc_menus += "<li><a href=\"" + Url.Content("~/Prod?cateb_id=" + cateb_id + "&cates_id=" + cates_id) + "\">" + cates_name + "</a></li>";
